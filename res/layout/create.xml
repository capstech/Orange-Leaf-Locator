	<ImageView android:id="@+id/topping_1" android:layout_width="wrap_content"
		android:layout_height="wrap_content" android:layout_centerHorizontal="true"
		android:duplicateParentState="false" android:visibility="visible" android:layout_marginBottom="55dip"></ImageView>
	<ImageView android:id="@+id/topping_2" android:layout_width="wrap_content"
		android:layout_height="wrap_content" android:layout_centerHorizontal="true"
		android:duplicateParentState="false" android:layout_marginBottom="55dip"
		android:visibility="visible"></ImageView>
	<ImageView android:id="@+id/topping_3" android:layout_width="wrap_content"
		android:layout_height="wrap_content" android:layout_centerHorizontal="true"
