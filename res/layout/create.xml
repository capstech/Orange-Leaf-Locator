					android:clickable="true"/>
				<ImageView android:tint="@color/red_velvet" android:id="@+id/flv_red_velvet"
					android:onClick="switchImages" android:layout_width="wrap_content"
					android:layout_height="wrap_content" android:src="@drawable/blank_flavor_26"
					android:clickable="true"/>
				<ImageView android:tint="@color/strawberry" android:id="@+id/flv_strawberry"
					android:onClick="switchImages" android:layout_width="wrap_content"
					android:layout_height="wrap_content" android:src="@drawable/blank_flavor_27"
					android:clickable="true"/>
				<ImageView android:tint="@color/taro" android:id="@+id/flv_taro"
					android:onClick="switchImages" android:layout_width="wrap_content"
					android:layout_height="wrap_content" android:src="@drawable/blank_flavor_28"
					android:clickable="true"/>
				<ImageView android:tint="@color/tart" android:id="@+id/flv_tart"
					android:onClick="switchImages" android:layout_width="wrap_content"
					android:layout_height="wrap_content" android:src="@drawable/blank_flavor_29"
