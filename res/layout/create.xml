					android:clickable="true" android:tint="@color/cheesecake"/>
				<ImageView android:id="@+id/flv_cherry"
					android:onClick="switchImages" android:layout_width="wrap_content"
					android:layout_height="wrap_content" android:src="@drawable/blank_flavor_4"
					android:clickable="true" android:tint="@color/cherry" />
				<ImageView android:tint="@color/chocolate" android:id="@+id/flv_chocolate"
					android:onClick="switchImages" android:layout_width="wrap_content"
