					android:onClick="switchToppings" />

				<ImageView android:id="@+id/hersheykiss"
					android:layout_width="wrap_content" android:layout_height="wrap_content"
					android:src="@drawable/hersheykiss" android:clickable="true"
					android:onClick="switchToppings" />

				<ImageView android:id="@+id/jellybean"
					android:layout_width="wrap_content" android:layout_height="wrap_content"
					android:src="@drawable/jellybean" android:clickable="true"
					android:onClick="switchToppings" />

				<ImageView android:id="@+id/kiwi" android:layout_width="wrap_content"
					android:layout_height="wrap_content" android:src="@drawable/kiwi"
					android:clickable="true" android:onClick="switchToppings" />

				<ImageView android:id="@+id/mango" android:layout_width="wrap_content"
					android:layout_height="wrap_content" android:src="@drawable/mango"
					android:clickable="true" android:onClick="switchToppings" />


				<ImageView android:id="@+id/marshmellow"
					android:layout_width="wrap_content" android:layout_height="wrap_content"
					android:src="@drawable/marshmellow" android:clickable="true"
					android:onClick="switchToppings" />

				<ImageView android:id="@+id/mm" android:layout_width="wrap_content"
					android:layout_height="wrap_content" android:src="@drawable/mm"
					android:clickable="true" android:onClick="switchToppings" />

				<ImageView android:id="@+id/mochio" android:layout_width="wrap_content"
					android:layout_height="wrap_content" android:src="@drawable/mochio"
					android:clickable="true" android:onClick="switchToppings" />

				<!--
					REMOVED <ImageView android:id="@+id/orange"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content" android:src="@drawable/orange"
					android:clickable="true" android:onClick="switchToppings" />
				-->

				<ImageView android:id="@+id/oreo" android:layout_width="wrap_content"
					android:layout_height="wrap_content" android:src="@drawable/oreo"
					android:clickable="true" android:onClick="switchToppings" />

				<ImageView android:id="@+id/peach" android:layout_width="wrap_content"
					android:layout_height="wrap_content" android:src="@drawable/peach"
					android:clickable="true" android:onClick="switchToppings" />

				<ImageView android:id="@+id/pear" android:layout_width="wrap_content"
					android:layout_height="wrap_content" android:src="@drawable/pear"
					android:clickable="true" android:onClick="switchToppings" />

				<ImageView android:id="@+id/pecan" android:layout_width="wrap_content"
					android:layout_height="wrap_content" android:src="@drawable/pecan"
					android:clickable="true" android:onClick="switchToppings" />

				<ImageView android:id="@+id/raspberry"
					android:layout_width="wrap_content" android:layout_height="wrap_content"
					android:src="@drawable/raspberry" android:clickable="true"
					android:onClick="switchToppings" />

				<ImageView android:id="@+id/reeses" android:layout_width="wrap_content"
					android:layout_height="wrap_content" android:src="@drawable/reeses"
					android:clickable="true" android:onClick="switchToppings" />


				<ImageView android:id="@+id/sprinkles"
					android:layout_width="wrap_content" android:layout_height="wrap_content"
					android:src="@drawable/sprinkles" android:clickable="true"
					android:onClick="switchToppings" />

				<ImageView android:id="@+id/strawberry"
					android:layout_width="wrap_content" android:layout_height="wrap_content"
					android:src="@drawable/strawberry" android:clickable="true"
					android:onClick="switchToppings" />

				<ImageView android:id="@+id/sunflower"
					android:layout_width="wrap_content" android:layout_height="wrap_content"
					android:src="@drawable/sunflower" android:clickable="true"
					android:onClick="switchToppings" />

				<ImageView android:id="@+id/watermelon"
					android:layout_width="wrap_content" android:layout_height="wrap_content"
					android:src="@drawable/watermelon" android:clickable="true"
					android:onClick="switchToppings" />
			</LinearLayout>
		</RelativeLayout>
	</ScrollView>
	<ScrollView android:layout_width="wrap_content"
		android:layout_height="wrap_content" android:id="@+id/ScrollViewleft"
		android:layout_centerHorizontal="true" android:scrollbars="none"
		android:layout_alignParentLeft="true" android:layout_marginBottom="15dip"
		android:layout_marginTop="15dip" android:layout_marginLeft="18dip"
		android:paddingBottom="17dip" android:paddingTop="17dip"
		android:background="@drawable/sidebars2" android:clickable="false">
