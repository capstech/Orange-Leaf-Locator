				<ImageView android:tint="@color/dole_pineapple"
					android:id="@+id/flv_dole_pineapple" android:onClick="switchImages"
					android:layout_width="wrap_content" android:layout_height="wrap_content"
					android:src="@drawable/blank_flavor_9" android:clickable="true"/>
				<ImageView android:tint="@color/eggnog" android:id="@+id/flv_eggnog"
					android:onClick="switchImages" android:layout_width="wrap_content"
					android:layout_height="wrap_content" android:src="@drawable/blank_flavor_10"
					android:clickable="true"/>
				<ImageView android:tint="@color/green_apple" android:id="@+id/flv_green_apple"
					android:onClick="switchImages" android:layout_width="wrap_content"
					android:layout_height="wrap_content" android:src="@drawable/blank_flavor_11"
					android:clickable="true"/>
				<ImageView android:tint="@color/green_tea" android:id="@+id/flv_green_tea"
					android:onClick="switchImages" android:layout_width="wrap_content"
					android:layout_height="wrap_content" android:src="@drawable/blank_flavor_12"
					android:clickable="true"/>
				<ImageView android:tint="@color/gingerbread" android:id="@+id/flv_gingerbread"
					android:onClick="switchImages" android:layout_width="wrap_content"
					android:layout_height="wrap_content" android:src="@drawable/blank_flavor_2"
					android:clickable="true"/>
				<ImageView android:tint="@color/honeydew" android:id="@+id/flv_honeydew"
					android:onClick="switchImages" android:layout_width="wrap_content"
					android:layout_height="wrap_content" android:src="@drawable/blank_flavor_13"
					android:clickable="true"/>
				<ImageView android:tint="@color/kiwi" android:id="@+id/flv_kiwi"
					android:onClick="switchImages" android:layout_width="wrap_content"
					android:layout_height="wrap_content" android:src="@drawable/blank_flavor_14"
					android:clickable="true"/>
				<ImageView android:tint="@color/lemon" android:id="@+id/flv_lemon"
					android:onClick="switchImages" android:layout_width="wrap_content"
					android:layout_height="wrap_content" android:src="@drawable/blank_flavor_15"
					android:clickable="true"/>
				<ImageView android:tint="@color/lychee" android:id="@+id/flv_lychee"
					android:onClick="switchImages" android:layout_width="wrap_content"
					android:layout_height="wrap_content" android:src="@drawable/blank_flavor_16"
					android:clickable="true"/>
				<ImageView android:tint="@color/mango_pomegranate"
					android:id="@+id/flv_mango_pomegranate" android:onClick="switchImages"
					android:layout_width="wrap_content" android:layout_height="wrap_content"
					android:src="@drawable/blank_flavor_17" android:clickable="true"/>
				<ImageView android:tint="@color/mint_chocolate"
					android:id="@+id/flv_mint_chocolate" android:onClick="switchImages"
					android:layout_width="wrap_content" android:layout_height="wrap_content"
					android:src="@drawable/blank_flavor_18" android:clickable="true"/>
				<ImageView android:tint="@color/orange" android:id="@+id/flv_orange"
					android:onClick="switchImages" android:layout_width="wrap_content"
					android:layout_height="wrap_content" android:src="@drawable/blank_flavor_19"
					android:clickable="true"/>
				<ImageView android:tint="@color/passion_fruit" android:id="@+id/flv_passion_fruit"
					android:onClick="switchImages" android:layout_width="wrap_content"
					android:layout_height="wrap_content" android:src="@drawable/blank_flavor_20"
