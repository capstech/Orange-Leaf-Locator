					android:clickable="true"/>
				<ImageView android:tint="@color/chocolate_raspberry"
					android:id="@+id/flv_chocolate_raspberry" android:onClick="switchImages"
					android:layout_width="wrap_content" android:layout_height="wrap_content"
					android:src="@drawable/blank_flavor_6" android:clickable="true"/>
				<ImageView android:tint="@color/coffee_lover" android:id="@+id/flv_coffee_lover"
					android:onClick="switchImages" android:layout_width="wrap_content"
					android:layout_height="wrap_content" android:src="@drawable/blank_flavor_7"
					android:clickable="true"/>
				<ImageView android:tint="@color/cookies_and_cream"
					android:id="@+id/flv_cookies_and_cream" android:onClick="switchImages"
