<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout android:id="@+id/RelativeLayout01"
	android:layout_width="fill_parent" android:layout_height="fill_parent"
	xmlns:android="http://schemas.android.com/apk/res/android"
	android:background="@drawable/create_bg">
	<ImageView android:id="@+id/layer_1_flavor"
		android:layout_width="wrap_content" android:layout_height="wrap_content"
		android:src="@drawable/icecream1" android:tint="@color/vanilla"
		android:layout_centerHorizontal="true"></ImageView>
	<ImageView android:id="@+id/layer_2_flavor"
		android:layout_width="wrap_content" android:layout_height="wrap_content"
		android:src="@drawable/icecream2" android:tint="@color/vanilla"
		android:layout_centerHorizontal="true"></ImageView>
	<ImageView android:id="@+id/layer_3_flavor"
		android:layout_width="wrap_content" android:layout_height="wrap_content"
		android:src="@drawable/icecream3" android:tint="@color/vanilla"
		android:layout_centerHorizontal="true"></ImageView>
	<ImageView android:id="@+id/layer_4_flavor"
		android:layout_width="wrap_content" android:layout_height="wrap_content"
		android:tint="@color/vanilla" android:layout_centerHorizontal="true"
		android:src="@drawable/icecream4"></ImageView>
	<ImageView android:id="@+id/layer_5_flavor"
		android:layout_width="wrap_content" android:layout_height="wrap_content"
		android:src="@drawable/icecream5" android:tint="@color/vanilla"
		android:layout_centerHorizontal="true"></ImageView>

	<ImageView android:id="@+id/main_cup" android:layout_width="wrap_content"
		android:layout_height="wrap_content" android:layout_centerHorizontal="true"
		android:src="@drawable/icecreamcup" android:layout_below="@drawable/icecream5"></ImageView>

	<ImageView android:id="@+id/main_cup_reflection"
		android:layout_width="wrap_content" android:layout_height="wrap_content"
		android:layout_centerHorizontal="true" android:src="@drawable/cupreflection"
		android:layout_alignParentBottom="false"></ImageView>


	<ImageView android:id="@+id/up_arrow" android:layout_width="wrap_content"
		android:layout_height="wrap_content" android:layout_centerHorizontal="true"
		android:src="@drawable/upbutton" android:layout_alignParentBottom="false"
		android:layout_alignParentTop="true" android:layout_marginTop="20dip"
		android:clickable="true" android:onClick="upLayer"></ImageView>


	<ImageView android:id="@+id/down_arrow" android:layout_width="wrap_content"
		android:layout_height="wrap_content" android:layout_centerHorizontal="true"
		android:src="@drawable/downbutton" android:layout_alignParentBottom="true"
		android:layout_alignParentTop="false" android:layout_marginBottom="20dip"
		android:clickable="true" android:onClick="downLayer"></ImageView>

	<ScrollView android:layout_width="wrap_content"
		android:layout_height="wrap_content" android:id="@+id/ScrollViewright"
		android:layout_centerHorizontal="true" android:clickable="true"
		android:scrollbars="none" android:layout_alignParentRight="true"
		android:layout_marginBottom="15dip" android:layout_marginTop="15dip"
		android:paddingBottom="17dip" android:paddingTop="17dip"
		android:layout_marginRight="18dip" android:background="@drawable/sidebars">
		<RelativeLayout android:id="@+id/topping_relative_layout"
			android:layout_width="fill_parent" android:layout_height="fill_parent"
			xmlns:android="http://schemas.android.com/apk/res/android">
			<LinearLayout android:id="@+id/topping_linear_layout"
				android:layout_width="wrap_content" android:layout_height="wrap_content"
				android:orientation="vertical" android:layout_centerInParent="true">
				<ImageView android:id="@+id/almonds" android:layout_width="wrap_content"
					android:layout_height="wrap_content" android:src="@drawable/almonds"
					android:clickable="true" android:onClick="switchToppings"></ImageView>
				<ImageView android:id="@+id/banana" android:layout_width="wrap_content"
					android:layout_height="wrap_content" android:src="@drawable/banana"
					android:clickable="true" android:onClick="switchToppings"></ImageView>
				<ImageView android:id="@+id/blackberry"
					android:layout_width="wrap_content" android:layout_height="wrap_content"
					android:src="@drawable/blackberry" android:clickable="true"
					android:onClick="switchToppings" />

				<ImageView android:id="@+id/blueberry"
					android:layout_width="wrap_content" android:layout_height="wrap_content"
					android:src="@drawable/blueberry" android:clickable="true"
					android:onClick="switchToppings" />

				<ImageView android:id="@+id/brownie" android:layout_width="wrap_content"
					android:layout_height="wrap_content" android:src="@drawable/brownie"
					android:clickable="true" android:onClick="switchToppings" />

				<ImageView android:id="@+id/butterfinger"
					android:layout_width="wrap_content" android:layout_height="wrap_content"
					android:src="@drawable/butterfinger" android:clickable="true"
					android:onClick="switchToppings" />


				<ImageView android:id="@+id/captaincrunch"
					android:layout_width="wrap_content" android:layout_height="wrap_content"
					android:src="@drawable/captaincrunch" android:clickable="true"
					android:onClick="switchToppings" />


				<ImageView android:id="@+id/cherries"
					android:layout_width="wrap_content" android:layout_height="wrap_content"
					android:src="@drawable/cherries" android:clickable="true"
					android:onClick="switchToppings" />

				<ImageView android:id="@+id/coconut" android:layout_width="wrap_content"
					android:layout_height="wrap_content" android:src="@drawable/coconut"
					android:clickable="true" android:onClick="switchToppings" />

				<ImageView android:id="@+id/cocopebbles"
					android:layout_width="wrap_content" android:layout_height="wrap_content"
					android:src="@drawable/cocopebbles" android:clickable="true"
					android:onClick="switchToppings" />

				<ImageView android:id="@+id/fruitypebbles"
					android:layout_width="wrap_content" android:layout_height="wrap_content"
